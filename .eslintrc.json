{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier",
        "prettier/react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "warn",
        "react/state-in-constructor": 0,
        "react/jsx-props-no-spreading": "off",
        "no-console": "off",
        "no-use-before-define": 0,
        "no-param-reassign": 1,
        "curly": "warn"
    }
}
