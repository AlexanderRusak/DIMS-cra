{
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/state-in-constructor": 0,
    "react/jsx-props-no-spreading": "off",
    "no-console": "off",
    "no-use-before-define": 0,
    "no-param-reassign": 1,
    "curly": "warn"
  }
}
